{
  "manifest_version" : 2,
  "name": "Site blocker",
  "description": "Allows user to add websites to a blacklist. Sites from this list are automatically closed.",
  "version": "0.9.0",
  "browser_action": {
      "default_icon": "icons/default_icon.png",
	    "default_popup": "html/popup.html"
   },
   "permissions": [ "storage", 
    "tabs" ],

    "background": {
      "scripts": ["js/background.js"]
    }
}